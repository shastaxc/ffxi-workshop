{
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify",
    "editor.formatOnSave": true,
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbeaumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "typescript"
  ]
}